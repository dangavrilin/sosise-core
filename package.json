{
  "name": "sosise-core",
  "version": "0.1.36",
  "description": "Sosise",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "/build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sosise/sosise-core"
  },
  "scripts": {
    "build": "rm -rf build && tsc && cp -R src/Artisan/FileTemplates build/Artisan/FileTemplates",
    "watch": "npm build && tsc -w",
    "pub": "npm version patch --no-git-tag-version && npm run build && npm publish",
    "cscheck": "./node_modules/.bin/tslint --project tsconfig.json",
    "eccheck": "./node_modules/.bin/editorconfig-checker -exclude \"(node_modules|build)\""
  },
  "author": "Igor Kov",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^5.29.2",
    "@sentry/tracing": "^5.29.2",
    "colors": "^1.4.0",
    "commander": "^6.2.1",
    "dayjs": "^1.10.2",
    "express": "^4.17.1",
    "find-process": "^1.4.4",
    "knex": "^0.21.15",
    "lodash": "^4.17.20",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/express": "^4.17.9",
    "@types/find-process": "^1.2.0",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.20",
    "editorconfig": "^0.15.3",
    "editorconfig-checker": "^3.3.0",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
